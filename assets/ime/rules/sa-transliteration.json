      "replacement": "$1ी"
    },
    {
      "pattern": "([क-ह]़?)्u",
      "replacement": "$1ु"
    },
    {
      "pattern": "([क-ह]़?)(ुu|्U|ॊo)",
      "replacement": "$1ू"
