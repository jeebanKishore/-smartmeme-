{
  "id": "ibb-tilde",
  "name": "ibb-tilde",
  "description": "Ibibio input keyboard",
  "date": "2023-04-03",
  "author": "Amir E. Aharoni",
  "license": "GPLv3",
  "version": "1.0",
  "rules": [
    {
      "pattern": "~A",
      "replacement": "Ʌ"
    },
    {
      "pattern": "~a",
      "replacement": "ʌ"
    },
    {
      "pattern": "~E",
      "replacement": "Ǝ"
    },
    {
      "pattern": "~e",
      "replacement": "ǝ"
    },
    {
      "pattern": "~I",
      "replacement": "Ị"
    },
    {
      "pattern": "~i",
      "replacement": "ị"
    },
    {
      "pattern": "~N",
      "replacement": "Ñ"
    },
    {
      "pattern": "~n",
      "replacement": "ñ"
    },
    {
      "pattern": "~O",
      "replacement": "Ọ"
    },
    {
      "pattern": "~o",
      "replacement": "ọ"
    },
    {
      "pattern": "~U",
      "replacement": "Ụ"
    },
    {
      "pattern": "~u",
      "replacement": "ụ"
    },
    {
      "pattern": "~\\\\",
      "replacement": "̀"
    },
    {
      "pattern": "~/",
      "replacement": "́"
    },
    {
      "pattern": "~\\^",
      "replacement": "̂"
    },
    {
      "pattern": "~-",
      "replacement": "̄"
    },
    {
      "pattern": "~v",
      "replacement": "̌"
    }
  ]
}