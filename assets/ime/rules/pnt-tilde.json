{
  "id": "pnt-tilde",
  "name": "Pontic tilde",
  "description": "Pontic tilde",
  "date": "2024-09-26",
  "author": "Amir E. Aharoni",
  "license": "GPLv3",
  "version": "1.0",
  "maxKeyLength": 2,
  "rules": [
    {
      "pattern": "~\"",
      "replacement": "̈"
    },
    {
      "pattern": "~\\^",
      "replacement": "̌"
    },
    {
      "pattern": "~:",
      "replacement": "̤"
    }
  ]
}