{
  "id": "efi-tilde",
  "name": "efi-tilde",
  "description": "Efik input keyboard",
  "date": "2022-05-23",
  "author": "Amir E. Aharoni",
  "license": "GPLv3",
  "version": "1.0",
  "rules": [
    {
      "pattern": "~E",
      "replacement": "Ẹ"
    },
    {
      "pattern": "~e",
      "replacement": "ẹ"
    },
    {
      "pattern": "~I",
      "replacement": "Ị"
    },
    {
      "pattern": "~i",
      "replacement": "ị"
    },
    {
      "pattern": "~N",
      "replacement": "N̄"
    },
    {
      "pattern": "~n",
      "replacement": "n̄"
    },
    {
      "pattern": "~O",
      "replacement": "Ọ"
    },
    {
      "pattern": "~o",
      "replacement": "ọ"
    },
    {
      "pattern": "~U",
      "replacement": "Ụ"
    },
    {
      "pattern": "~u",
      "replacement": "ụ"
    },
    {
      "pattern": "~\\\\",
      "replacement": "̀"
    },
    {
      "pattern": "~/",
      "replacement": "́"
    }
  ]
}