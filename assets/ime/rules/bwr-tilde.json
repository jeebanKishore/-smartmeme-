{
  "id": "bwr-tilde",
  "name": "Bura tilde",
  "description": "Bura tilde keyboard",
  "date": "2022-08-14",
  "author": "Amir E. Aharoni",
  "license": "GPLv3",
  "version": "1.1",
  "maxKeyLength": 2,
  "rules": [
    {
      "pattern": "~B",
      "replacement": "Ɓ"
    },
    {
      "pattern": "~b",
      "replacement": "ɓ"
    },
    {
      "pattern": "~D",
      "replacement": "Ɗ"
    },
    {
      "pattern": "~d",
      "replacement": "ɗ"
    },
    {
      "pattern": "~E",
      "replacement": "Ə"
    },
    {
      "pattern": "~e",
      "replacement": "ə"
    },
    {
      "pattern": "~K",
      "replacement": "Ƙ"
    },
    {
      "pattern": "~k",
      "replacement": "ƙ"
    }
  ]
}