{
  "id": "dua-tilde",
  "name": "Duala tilde",
  "description": "Duala tilde keyboard",
  "date": "2024-08-09",
  "author": "Amir E. Aharoni",
  "license": "GPLv3",
  "version": "1.0",
  "maxKeyLength": 2,
  "rules": [
    {
      "pattern": "~B",
      "replacement": "Ɓ"
    },
    {
      "pattern": "~b",
      "replacement": "ɓ"
    },
    {
      "pattern": "~D",
      "replacement": "Ɗ"
    },
    {
      "pattern": "~d",
      "replacement": "ɗ"
    },
    {
      "pattern": "~E",
      "replacement": "Ɛ"
    },
    {
      "pattern": "~e",
      "replacement": "ɛ"
    },
    {
      "pattern": "~N",
      "replacement": "Ŋ"
    },
    {
      "pattern": "~n",
      "replacement": "ŋ"
    },
    {
      "pattern": "~j",
      "replacement": "ɲ"
    },
    {
      "pattern": "~J",
      "replacement": "Ɲ"
    },
    {
      "pattern": "~O",
      "replacement": "Ɔ"
    },
    {
      "pattern": "~o",
      "replacement": "ɔ"
    },
    {
      "pattern": "~\\\\",
      "replacement": "̀"
    },
    {
      "pattern": "~/",
      "replacement": "́"
    },
    {
      "pattern": "~\\^",
      "replacement": "̂"
    },
    {
      "pattern": "~v",
      "replacement": "̌"
    }
  ]
}