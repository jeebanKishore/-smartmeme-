{
  "id": "st-tilde",
  "name": "st-tilde",
  "description": "Sotho tilde keyboard",
  "date": "2019-03-28",
  "author": "Amir E. Aharoni",
  "license": "GPLv3",
  "version": "1.0",
  "rules": [
    {
      "pattern": "~S",
      "replacement": "Š"
    },
    {
      "pattern": "~s",
      "replacement": "š"
    },
    {
      "pattern": "~\\\\",
      "replacement": "̀"
    },
    {
      "pattern": "~\\-",
      "replacement": "̄"
    }
  ]
}