{
  "id": "laj-tilde",
  "name": "Lango tilde",
  "description": "Lango tilde",
  "date": "2024-09-30",
  "author": "Amir E. Aharoni",
  "license": "GPLv3",
  "version": "1.0",
  "maxKeyLength": 2,
  "rules": [
    {
      "pattern": "~E",
      "replacement": "Ë"
    },
    {
      "pattern": "~e",
      "replacement": "ë"
    },
    {
      "pattern": "~I",
      "replacement": "Ï"
    },
    {
      "pattern": "~i",
      "replacement": "ï"
    },
    {
      "pattern": "~N",
      "replacement": "Ŋ"
    },
    {
      "pattern": "~n",
      "replacement": "ŋ"
    },
    {
      "pattern": "~O",
      "replacement": "Ö"
    },
    {
      "pattern": "~o",
      "replacement": "ö"
    },
    {
      "pattern": "~U",
      "replacement": "Ü"
    },
    {
      "pattern": "~u",
      "replacement": "ü"
    }
  ]
}