{
  "id": "gaa-tilde",
  "name": "Ga tilde",
  "description": "Ga tilde",
  "date": "2019-05-06",
  "author": "Amir E. Aharoni",
  "license": "GPLv3",
  "version": "1.1",
  "maxKeyLength": 2,
  "rules": [
    {
      "pattern": "~E",
      "replacement": "Ɛ"
    },
    {
      "pattern": "~e",
      "replacement": "ɛ"
    },
    {
      "pattern": "~N",
      "replacement": "Ŋ"
    },
    {
      "pattern": "~n",
      "replacement": "ŋ"
    },
    {
      "pattern": "~O",
      "replacement": "Ɔ"
    },
    {
      "pattern": "~o",
      "replacement": "ɔ"
    },
    {
      "pattern": "~\\\\",
      "replacement": "̀"
    },
    {
      "pattern": "~/",
      "replacement": "́"
    },
    {
      "pattern": "~\\{",
      "replacement": "̃"
    }
  ]
}