{
  "id": "pa-transliteration",
  "name": "Punjabi Transliteration",
  "description": "Punjabi transliteration",
  "date": "2012-10-16",
  "author": "Amir E. Aharoni, inputs from Saurabh Choudhary and Surinder Wadhawan",
  "license": "GPLv3",
  "version": "1.0",
  "contextLength": 2,
  "maxKeyLength": 4,
  "rules": [
    {
      "pattern": "ਕ੍h",
      "replacement": "c"
    },
    {
      "pattern": "\\\\([A-Za-z\\>_~\\.0-9])",
      "replacement": "\\\\"
    },
    {
      "pattern": "([ਕ-ਹੜ]਼?)੍a",
      "replacement": "$1"
    },
    {
      "pattern": "([ਕ-ਹੜ]਼?)੍A",
      "replacement": "$1ਾ"
    },
    {
      "pattern": "([ਕ-ਹੜ]਼?)a",
      "replacement": "$1ਾ"
    },
    {
      "pattern": "([ਕ-ਹੜ]਼?)੍i",
      "replacement": "$1ਿ"
    },
    {
      "pattern": "([ਕ-ਹੜ]਼?)(ਿi|੍I|ੇe)",
      "replacement": "$1ੀ"
    },
    {
      "pattern": "([ਕ-ਹੜ]਼?)੍u",
      "replacement": "$1ੁ"
    },
    {
      "pattern": "([ਕ-ਹੜ]਼?)(ੁu|੍U|ੋo)",
      "replacement": "$1ੂ"
    },
    {
      "pattern": "([ਕ-ਹੜ]਼?)੍e",
      "replacement": "$1ੇ"
    },
    {
      "pattern": "([ਕ-ਹੜ]਼?)(i|੍E)",
      "replacement": "$1ੈ"
    },
    {
      "pattern": "([ਕ-ਹੜ]਼?)੍[oO]",
      "replacement": "$1ੋ"
    },
    {
      "pattern": "([ਕ-ਹੜ]਼?)u",
      "replacement": "$1ੌ"
    },
    {
      "pattern": "([ਕ-ਹੜ])੍\\`",
      "replacement": "$1਼੍"
    },
    {
      "pattern": "ਅa",
      "replacement": "ਆ"
    },
    {
      "pattern": "(ਓo|ਉu)",
      "replacement": "ਊ"
    },
    {
      "pattern": "ਅi",
      "replacement": "ਐ"
    },
    {
      "pattern": "ਅ\\^",
      "replacement": "ੲ"
    },
    {
      "pattern": "ਉ\\^",
      "replacement": "ੳ"
    },
    {
      "pattern": "(ਏe|ਇi)",
      "replacement": "ਈ"
    },
    {
      "pattern": "ਅu",
      "replacement": "ਔ"
    },
    {
      "pattern": "ਂ[Mm^]",
      "replacement": "ਁ"
    },
    {
      "pattern": "ਣ੍N",
      "replacement": "ੰ"
    },
    {
