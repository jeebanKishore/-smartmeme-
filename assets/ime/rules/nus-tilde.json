{
  "id": "nus-tilde",
  "name": "nus-tilde",
  "description": "Nuer input keyboard",
  "date": "2021-01-18",
  "author": "Amir E. Aharoni",
  "license": "GPLv3",
  "version": "1.0",
  "rules": [
    {
      "pattern": "~E",
      "replacement": "Ɛ"
    },
    {
      "pattern": "~e",
      "replacement": "ɛ"
    },
    {
      "pattern": "~G",
      "replacement": "Ɣ"
    },
    {
      "pattern": "~g",
      "replacement": "ɣ"
    },
    {
      "pattern": "~N",
      "replacement": "Ŋ"
    },
    {
      "pattern": "~n",
      "replacement": "ŋ"
    },
    {
      "pattern": "~O",
      "replacement": "Ɔ"
    },
    {
      "pattern": "~o",
      "replacement": "ɔ"
    },
    {
      "pattern": "~:",
      "replacement": "̈"
    },
    {
      "pattern": "~_",
      "replacement": "̱"
    }
  ]
}