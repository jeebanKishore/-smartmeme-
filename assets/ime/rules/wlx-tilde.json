{
  "id": "wlx-tilde",
  "name": "Waale tilde",
  "description": "Waale tilde",
  "date": "2024-07-03",
  "author": "Amir E. Aharoni",
  "license": "GPLv3",
  "version": "1.0",
  "maxKeyLength": 2,
  "rules": [
    {
      "pattern": "~E",
      "replacement": "Ɛ"
    },
    {
      "pattern": "~e",
      "replacement": "ɛ"
    },
    {
      "pattern": "~I",
      "replacement": "Ɩ"
    },
    {
      "pattern": "~i",
      "replacement": "ɩ"
    },
    {
      "pattern": "~N",
      "replacement": "Ŋ"
    },
    {
      "pattern": "~n",
      "replacement": "ŋ"
    },
    {
      "pattern": "~O",
      "replacement": "Ɔ"
    },
    {
      "pattern": "~o",
      "replacement": "ɔ"
    },
    {
      "pattern": "~U",
      "replacement": "Ʋ"
    },
    {
      "pattern": "~u",
      "replacement": "ʋ"
    }
  ]
}