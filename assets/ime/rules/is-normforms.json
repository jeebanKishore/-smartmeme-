    {
      "pattern": "TH",
      "replacement": "Þ"
    },
    {
      "pattern": "Th",
      "replacement": "Þ"
    },
    {
      "pattern": "aa",
      "replacement": "å"
    },
    {
      "pattern": "AA",
      "replacement": "Å"
