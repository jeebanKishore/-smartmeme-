    {
      "pattern": "þh",
      "replacement": "h"
    },
    {
      "pattern": "ÞA",
      "replacement": "H"
    },
    {
      "pattern": "Þh",
      "replacement": "H"
    },
    {
      "pattern": "þA",
      "replacement": "h"
    },
    {
      "pattern": "åa",
      "replacement": "a"
