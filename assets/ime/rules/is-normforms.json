{
  "id": "is-normforms",
  "name": "Íslenska",
  "description": "Islandic input method with most common form transliterated",
  "date": "2012-12-04",
  "author": "John Erling Blad",
  "license": "GPLv3",
  "version": "1.0",
  "contextLength": 1,
  "maxKeyLength": 3,
