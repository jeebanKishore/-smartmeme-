{
  "id": "be-latin",
  "name": "Belarusian Łacinka",
  "description": "Belarusian Latin alphabet input method",
  "date": "2012-11-06",
  "author": "Pavel Selitskas",
  "license": "GPLv3",
  "version": "1.0",
  "contextLength": 1,
  "maxKeyLength": 1,
  "rules": [
    {
      "pattern": "=S",
      "replacement": "Š"
    },
    {
      "pattern": "=U",
      "replacement": "Ŭ"
    },
    {
      "pattern": "-S",
      "replacement": "Ś"
    },
    {
      "pattern": "-L",
      "replacement": "Ł"
    },
    {
      "pattern": "-Z",
      "replacement": "Ź"
    },
    {
      "pattern": "=Z",
      "replacement": "Ž"
    },
    {
      "pattern": "-C",
      "replacement": "Ć"
    },
    {
      "pattern": "=C",
      "replacement": "Č"
    },
    {
      "pattern": "-N",
      "replacement": "Ń"
    },
    {
      "pattern": "=s",
      "replacement": "š"
    },
    {
      "pattern": "=u",
      "replacement": "ŭ"
    },
    {
      "pattern": "-s",
      "replacement": "ś"
    },
    {
      "pattern": "-l",
      "replacement": "ł"
    },
    {
      "pattern": "-z",
      "replacement": "ź"
    },
    {
      "pattern": "=z",
      "replacement": "ž"
    },
    {
      "pattern": "-c",
      "replacement": "ć"
    },
    {
      "pattern": "=c",
      "replacement": "č"
    },
    {
      "pattern": "-n",
      "replacement": "ń"
    }
  ]
}