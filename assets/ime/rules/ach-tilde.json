{
  "id": "ach-tilde",
  "name": "ach-tilde",
  "description": "Acholi tilde keyboard",
  "date": "2024-09-30",
  "author": "Amir E. Aharoni",
  "license": "GPLv3",
  "version": "1.0",
  "rules": [
    {
      "pattern": "~N",
      "replacement": "Ŋ"
    },
    {
      "pattern": "~n",
      "replacement": "ŋ"
    },
    {
      "pattern": "~/",
      "replacement": "́"
    },
    {
      "pattern": "~\\\\",
      "replacement": "̀"
    }
  ]
}