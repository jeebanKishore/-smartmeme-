{
  "id": "kcg-tilde",
  "name": "kcg-tilde",
  "description": "Tyap input keyboard",
  "date": "2020-12-04",
  "author": "Amir E. Aharoni",
  "license": "GPLv3",
  "version": "1.1",
  "rules": [
    {
      "pattern": "~A",
      "replacement": "A̱"
    },
    {
      "pattern": "~a",
      "replacement": "a̱"
    },
    {
      "pattern": "~I",
      "replacement": "I̱"
    },
    {
      "pattern": "~i",
      "replacement": "i̱"
    },
    {
      "pattern": "~/",
      "replacement": "́"
    },
    {
      "pattern": "~_",
      "replacement": "̱"
    }
  ]
}