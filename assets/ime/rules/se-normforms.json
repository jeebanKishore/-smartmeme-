{
  "id": "se-normforms",
  "name": "Davvisámegiella",
  "description": "Northern Sami input method",
  "date": "2012-12-04",
  "author": "John Erling Blad",
  "license": "GPLv3",
  "version": "1.0",
  "maxKeyLength": 3,
  "rules": [
    {
      "pattern": "'a",
      "replacement": "á"
    },
    {
      "pattern": "'A",
      "replacement": "Á"
    },
    {
      "pattern": "~c",
      "replacement": "č"
    },
    {
      "pattern": "~C",
      "replacement": "Č"
    },
    {
      "pattern": "~d",
      "replacement": "đ"
    },
    {
      "pattern": "~D",
      "replacement": "Đ"
    },
    {
      "pattern": "~g",
      "replacement": "ŋ"
    },
    {
      "pattern": "~G",
      "replacement": "Ŋ"
    },
    {
      "pattern": "~s",
      "replacement": "š"
    },
    {
      "pattern": "~S",
      "replacement": "Š"
    },
    {
      "pattern": "~t",
      "replacement": "ŧ"
    },
    {
      "pattern": "~T",
      "replacement": "Ŧ"
    },
    {
      "pattern": "~z",
      "replacement": "ž"
    },
    {
      "pattern": "~Z",
      "replacement": "Ž"
    }
  ]
}