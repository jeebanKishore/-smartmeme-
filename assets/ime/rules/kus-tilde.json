{
  "id": "kus-tilde",
  "name": "Kusaal tilde",
  "description": "Kusaal input keyboard",
  "date": "2022-12-11",
  "author": "Amir E. Aharoni",
  "license": "GPLv3",
  "version": "1.0",
  "rules": [
    {
      "pattern": "~E",
      "replacement": "Ɛ"
    },
    {
      "pattern": "~e",
      "replacement": "ɛ"
    },
    {
      "pattern": "~I",
      "replacement": "Ɩ"
    },
    {
      "pattern": "~i",
      "replacement": "ɩ"
    },
    {
      "pattern": "~N",
      "replacement": "Ŋ"
    },
    {
      "pattern": "~n",
      "replacement": "ŋ"
    },
    {
      "pattern": "~O",
      "replacement": "Ɔ"
    },
    {
      "pattern": "~o",
      "replacement": "ɔ"
    },
    {
      "pattern": "~U",
      "replacement": "Ʋ"
    },
    {
      "pattern": "~u",
      "replacement": "ʋ"
    },
    {
      "pattern": "~\\\\",
      "replacement": "̀"
    },
    {
      "pattern": "~/",
      "replacement": "́"
    },
    {
      "pattern": "~\\{",
      "replacement": "̃"
    },
    {
      "pattern": "~-",
      "replacement": "̄"
    }
  ]
}