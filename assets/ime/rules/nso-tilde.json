{
  "id": "nso-tilde",
  "name": "nso-tilde",
  "description": "Northern Sotho input keyboard",
  "date": "2018-12-02",
  "author": "Amir E. Aharoni",
  "license": "GPLv3",
  "version": "1.0",
  "rules": [
    {
      "pattern": "~S",
      "replacement": "Š"
    },
    {
      "pattern": "~s",
      "replacement": "š"
    },
    {
      "pattern": "~E",
      "replacement": "Ê"
    },
    {
      "pattern": "~e",
      "replacement": "ê"
    },
    {
      "pattern": "~O",
      "replacement": "Ô"
    },
    {
      "pattern": "~o",
      "replacement": "ô"
    }
  ]
}