{
  "id": "mul-bf",
  "name": "Burkina Faso tilde",
  "description": "Burkina Faso tilde keyboard",
  "date": "2022-02-14",
  "author": "Amir E. Aharoni",
  "license": "GPLv3",
  "version": "1.0",
  "maxKeyLength": 2,
  "rules": [
    {
      "pattern": "~B",
      "replacement": "Ɓ"
    },
    {
      "pattern": "~b",
      "replacement": "ɓ"
    },
    {
      "pattern": "~C",
      "replacement": "Ç"
    },
    {
      "pattern": "~c",
      "replacement": "ç"
    },
    {
      "pattern": "~D",
      "replacement": "Ɗ"
    },
    {
      "pattern": "~d",
      "replacement": "ɗ"
    },
    {
      "pattern": "~A",
      "replacement": "Ǝ"
    },
    {
      "pattern": "~a",
      "replacement": "ǝ"
    },
    {
      "pattern": "~E",
      "replacement": "Ɛ"
    },
    {
      "pattern": "~e",
      "replacement": "ɛ"
    },
    {
      "pattern": "~I",
      "replacement": "Ɩ"
    },
    {
      "pattern": "~i",
      "replacement": "ɩ"
    },
    {
      "pattern": "~N",
      "replacement": "Ŋ"
    },
    {
      "pattern": "~n",
      "replacement": "ŋ"
    },
    {
      "pattern": "~O",
      "replacement": "Ɔ"
    },
    {
      "pattern": "~o",
      "replacement": "ɔ"
    },
    {
      "pattern": "~U",
      "replacement": "Ʋ"
    },
    {
      "pattern": "~u",
      "replacement": "ʋ"
    },
    {
      "pattern": "~W",
      "replacement": "Ⱳ"
    },
    {
      "pattern": "~w",
      "replacement": "ⱳ"
    },
    {
      "pattern": "~Y",
      "replacement": "Ƴ"
    },
    {
      "pattern": "~y",
      "replacement": "ƴ"
    },
    {
      "pattern": "~\\\\",
      "replacement": "̀"
    },
    {
      "pattern": "~/",
      "replacement": "́"
    },
    {
      "pattern": "~\\^",
      "replacement": "̂"
    },
    {
      "pattern": "~\\{",
      "replacement": "̃"
    },
    {
      "pattern": "~-",
      "replacement": "̄"
    },
    {
      "pattern": "~:",
      "replacement": "̈"
    },
    {
      "pattern": "~v",
      "replacement": "̌"
    }
  ]
}