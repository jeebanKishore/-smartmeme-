{
  "id": "ak-tilde",
  "name": "Akan tilde",
  "description": "Akan tilde",
  "date": "2019-05-06",
  "author": "Amir E. Aharoni",
  "license": "GPLv3",
  "version": "1.0",
  "maxKeyLength": 2,
  "rules": [
    {
      "pattern": "~E",
      "replacement": "Ɛ"
    },
    {
      "pattern": "~e",
      "replacement": "ɛ"
    },
    {
      "pattern": "~O",
      "replacement": "Ɔ"
    },
    {
      "pattern": "~o",
      "replacement": "ɔ"
    }
  ]
}