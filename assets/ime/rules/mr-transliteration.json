{
  "id": "mr-transliteration",
  "name": "अक्षरांतरण",
  "description": "Transliteration keyboard for Marathi script",
  "date": "2012-10-14",
  "author": "Pathak A B",
  "license": "GPLv3",
  "version": "1.0",
  "contextLength": 2,
  "maxKeyLength": 4,
  "rules": [
    {
      "pattern": "च्h",
      "replacement": "c"
    },
    {
      "pattern": "च्h",
      "replacement": "ch"
    },
    {
      "pattern": "\\\\([A-Za-z\\>_~0-9])",
      "replacement": "\\\\"
    },
    {
      "pattern": "([क-ह]़?)्a",
      "replacement": "$1"
    },
    {
      "pattern": "([क-ह]़?)्A",
      "replacement": "$1ा"
    },
    {
      "pattern": "([क-ह]़?)a",
      "replacement": "$1ा"
    },
    {
      "pattern": "([क-ह]़?)्i",
      "replacement": "$1ि"
    },
    {
      "pattern": "([क-ह]़?)(्I|िi|ेe)",
      "replacement": "$1ी"
    },
    {
      "pattern": "([क-ह]़?)्u",
      "replacement": "$1ु"
    },
    {
      "pattern": "([क-ह]़?)(ुu|्U|ोo)",
      "replacement": "$1ू"
    },
    {
      "pattern": "([क-ह]़?)्R",
      "replacement": "$1ृ"
    },
    {
      "pattern": "([क-ह]़?)ृR",
      "replacement": "$1ॄ"
    },
    {
      "pattern": "([क-ह]़?)्ळ्l",
      "replacement": "$1ॢ"
    },
    {
      "pattern": "([क-ह]़?)ॢl",
      "replacement": "$1ॣ"
    },
    {
      "pattern": "([क-ह]़?)े\\^",
      "replacement": "$1ॅ"
    },
    {
      "pattern": "([क-ह]़?)्e",
      "replacement": "$1े"
    },
    {
      "pattern": "([क-ह]़?)्E",
      "replacement": "$1ॅ"
    },
    {
      "pattern": "([क-ह]़?)ो\\^",
      "replacement": "$1ॉ"
    },
    {
      "pattern": "([क-ह]़?)i",
      "replacement": "$1ै"
    },
    {
      "pattern": "([क-ह]़?)्o",
      "replacement": "$1ो"
    },
    {
      "pattern": "([क-ह]़?)्O",
      "replacement": "$1ॉ"
    },
    {
      "pattern": "([क-ह]़?)u",
      "replacement": "$1ौ"
    },
    {
      "pattern": "([क-ह])्\\`",
      "replacement": "$1़्"
    },
    {
      "pattern": "([क-ह])ॉM",
      "replacement": "$1ाँ"
    },
    {
      "pattern": "आऊM",
      "replacement": "ॐ"
    },
    {
      "pattern": "(द्न्y|ग्ग्y|ज्ज्n)",
      "replacement": "ज्ञ्"
