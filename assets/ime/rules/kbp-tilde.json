{
  "id": "kbp-tilde",
  "name": "Kabɩyɛ tilde",
  "description": "Kabiye input keyboard",
  "date": "2018-12-18",
  "author": "Amir E. Aharoni",
  "license": "GPLv3",
  "version": "1.0",
  "rules": [
    {
      "pattern": "~D",
      "replacement": "Ɖ"
    },
    {
      "pattern": "~d",
      "replacement": "ɖ"
    },
    {
      "pattern": "~E",
      "replacement": "Ɛ"
    },
    {
      "pattern": "~e",
      "replacement": "ɛ"
    },
    {
      "pattern": "~G",
      "replacement": "Ɣ"
    },
    {
      "pattern": "~g",
      "replacement": "ɣ"
    },
    {
      "pattern": "~I",
      "replacement": "Ɩ"
    },
    {
      "pattern": "~i",
      "replacement": "ɩ"
    },
    {
      "pattern": "~N",
      "replacement": "Ŋ"
    },
    {
      "pattern": "~n",
      "replacement": "ŋ"
    },
    {
      "pattern": "~O",
      "replacement": "Ɔ"
    },
    {
      "pattern": "~o",
      "replacement": "ɔ"
    },
    {
      "pattern": "~V",
      "replacement": "Ʋ"
    },
    {
      "pattern": "~v",
      "replacement": "ʋ"
    },
    {
      "pattern": "~/",
      "replacement": "́"
    },
    {
      "pattern": "~\\\\",
      "replacement": "̀"
    },
    {
      "pattern": "~\\{",
      "replacement": "̃"
    }
  ]
}