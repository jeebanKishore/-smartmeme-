{
  "id": "ki-tilde",
  "name": "ki-tilde",
  "description": "Kikuyu input keyboard - tilde",
  "date": "2019-01-22",
  "author": "Amir E. Aharoni",
  "license": "GPLv3",
  "version": "1.0",
  "rules": [
    {
      "pattern": "~I",
      "replacement": "Ĩ"
    },
    {
      "pattern": "~i",
      "replacement": "ĩ"
    },
    {
      "pattern": "~U",
      "replacement": "Ũ"
    },
    {
      "pattern": "~u",
      "replacement": "ũ"
    }
  ]
}