{
  "id": "fi-transliteration",
  "name": "translitterointi",
  "description": "Finnish transliteration",
  "date": "2012-11-10",
  "author": "Niklas Laxström",
  "license": "GPLv3",
  "version": "1.0",
  "contextLength": 0,
  "maxKeyLength": 2,
  "rules": [
    {
      "pattern": "\\.a",
      "replacement": "å"
    },
    {
      "pattern": "\\.A",
      "replacement": "Å"
    },
    {
      "pattern": ":a",
      "replacement": "ä"
    },
    {
      "pattern": ":A",
      "replacement": "Ä"
    },
    {
      "pattern": ":o",
      "replacement": "ö"
    },
    {
      "pattern": ":O",
      "replacement": "Ö"
    },
    {
      "pattern": "shh",
      "replacement": "š"
    },
    {
      "pattern": "Shh",
      "replacement": "Š"
    },
    {
      "pattern": "\\.e",
      "replacement": "€"
    },
    {
      "pattern": "e",
      "replacement": "€",
      "context": "x"
    }
  ]
}