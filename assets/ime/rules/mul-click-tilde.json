{
  "id": "mul-click-tilde",
  "name": "Click tilde",
  "description": "Click input keyboard",
  "date": "2024-09-24",
  "author": "Amir E. Aharoni",
  "license": "GPLv3",
  "version": "1.0",
  "rules": [
    {
      "pattern": "~o",
      "replacement": "ʘ"
    },
    {
      "pattern": "~1",
      "replacement": "ǀ"
    },
    {
      "pattern": "ǀ1",
      "replacement": "ǁ"
    },
    {
      "pattern": "~=",
      "replacement": "ǂ"
    },
    {
      "pattern": "~#",
      "replacement": "ǂ"
    },
    {
      "pattern": "~!",
      "replacement": "ǃ"
    },
    {
      "pattern": "~,",
      "replacement": "𝼊"
    },
    {
      "pattern": "~c",
      "replacement": "ǀ"
    },
    {
      "pattern": "~x",
      "replacement": "ǁ"
    },
    {
      "pattern": "~q",
      "replacement": "ǃ"
    },
    {
      "pattern": "~\\\\",
      "replacement": "̀"
    },
    {
      "pattern": "~/",
      "replacement": "́"
    },
    {
      "pattern": "~\\^",
      "replacement": "̂"
    },
    {
      "pattern": "~\\{",
      "replacement": "̃"
    },
    {
      "pattern": "~-",
      "replacement": "̄"
    }
  ]
}