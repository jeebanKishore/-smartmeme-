{
  "id": "sg-tilde",
  "name": "sg-tilde",
  "description": "Sango tilde keyboard",
  "date": "2019-03-28",
  "author": "Amir E. Aharoni",
  "license": "GPLv3",
  "version": "1.0",
  "rules": [
    {
      "pattern": "~\\\\",
      "replacement": "̀"
    },
    {
      "pattern": "~/",
      "replacement": "́"
    },
    {
      "pattern": "~\\^",
      "replacement": "̂"
    },
    {
      "pattern": "~\\-",
      "replacement": "̄"
    },
    {
      "pattern": "~\\:",
      "replacement": "̈"
    },
    {
      "pattern": "~v",
      "replacement": "̌"
    }
  ]
}