{
  "id": "fon-tilde",
  "name": "fon-tilde",
  "description": "Fon input keyboard",
  "date": "2018-05-18",
  "author": "Mahuton POSSOUPE, Amir E. Aharoni",
  "license": "GPLv3",
  "version": "1.1",
  "rules": [
    {
      "pattern": "~D",
      "replacement": "Ɖ"
    },
    {
      "pattern": "~d",
      "replacement": "ɖ"
    },
    {
      "pattern": "~E",
      "replacement": "Ɛ"
    },
    {
      "pattern": "~e",
      "replacement": "ɛ"
    },
    {
      "pattern": "~O",
      "replacement": "Ɔ"
    },
    {
      "pattern": "~o",
      "replacement": "ɔ"
    },
    {
      "pattern": "~\\\\",
      "replacement": "̀"
    },
    {
      "pattern": "~/",
      "replacement": "́"
    },
    {
      "pattern": "~\\^",
      "replacement": "̂"
    },
    {
      "pattern": "~-",
      "replacement": "̄"
    },
    {
      "pattern": "~v",
      "replacement": "̌"
    }
  ]
}