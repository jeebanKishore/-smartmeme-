{
  "id": "mg-tilde",
  "name": "mg-tilde",
  "description": "Malagasy tilde keyboard",
  "date": "2019-03-28",
  "author": "Amir E. Aharoni",
  "license": "GPLv3",
  "version": "1.0",
  "rules": [
    {
      "pattern": "~n",
      "replacement": "n̈"
    },
    {
      "pattern": "~N",
      "replacement": "N̈"
    },
    {
      "pattern": "~\\\\",
      "replacement": "̀"
    },
    {
      "pattern": "~/",
      "replacement": "́"
    },
    {
      "pattern": "~\\^",
      "replacement": "̂"
    },
    {
      "pattern": "~\\:",
      "replacement": "̈"
    },
    {
      "pattern": "~v",
      "replacement": "̌"
    }
  ]
}