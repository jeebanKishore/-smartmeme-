{
  "id": "fo-normforms",
  "name": "Føroyskt",
  "description": "Faroese input method with most common form transliterated",
  "date": "2012-12-04",
  "author": "John Erling Blad",
  "license": "GPLv3",
  "version": "1.0",
  "contextLength": 1,
  "maxKeyLength": 3,
  "rules": [
    {
      "pattern": "'a",
      "replacement": "á"
    },
    {
      "pattern": "'A",
      "replacement": "Á"
    },
    {
      "pattern": "'i",
      "replacement": "í"
    },
    {
      "pattern": "'I",
      "replacement": "Í"
    },
    {
      "pattern": "'o",
      "replacement": "ó"
    },
    {
      "pattern": "'O",
      "replacement": "Ó"
    },
    {
      "pattern": "'u",
      "replacement": "ú"
    },
    {
      "pattern": "'U",
      "replacement": "Ú"
    },
    {
      "pattern": "'y",
      "replacement": "ý"
    },
    {
      "pattern": "'Y",
      "replacement": "Ý"
    },
    {
      "pattern": "~d",
      "replacement": "ð"
    },
    {
      "pattern": "~D",
      "replacement": "Ð"
    },
    {
      "pattern": "~o",
      "replacement": "ö"
    },
    {
      "pattern": "~O",
      "replacement": "Ö"
    },
    {
      "pattern": "aa",
      "replacement": "å"
    },
    {
      "pattern": "AA",
      "replacement": "Å"
    },
    {
      "pattern": "Aa",
      "replacement": "Å"
    },
    {
      "pattern": "ae",
      "replacement": "æ"
    },
    {
      "pattern": "AE",
      "replacement": "Æ"
    },
    {
      "pattern": "Ae",
      "replacement": "Æ"
    },
    {
      "pattern": "oe",
      "replacement": "ø"
    },
    {
      "pattern": "OE",
      "replacement": "Ø"
    },
    {
      "pattern": "Oe",
      "replacement": "Ø"
    },
    {
      "pattern": "åa",
      "replacement": "a"
    },
    {
      "pattern": "ÅA",
      "replacement": "A"
    },
    {
      "pattern": "Åa",
      "replacement": "A"
    },
    {
      "pattern": "åA",
      "replacement": "a"
    },
    {
      "pattern": "æe",
      "replacement": "e"
    },
    {
      "pattern": "ÆE",
      "replacement": "E"
    },
    {
      "pattern": "Æe",
      "replacement": "E"
    },
    {
      "pattern": "æE",
      "replacement": "e"
    },
    {
      "pattern": "øe",
      "replacement": "e"
    },
    {
      "pattern": "ØE",
      "replacement": "E"
    },
    {
      "pattern": "Øe",
      "replacement": "E"
    },
    {
      "pattern": "øE",
      "replacement": "e"
    }
  ]
}