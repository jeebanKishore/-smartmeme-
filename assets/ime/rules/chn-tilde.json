{
  "id": "chn-tilde",
  "name": "chn-tilde",
  "description": "Chinook Jargon input keyboard - tilde",
  "date": "2024-02-07",
  "author": "Amir E. Aharoni",
  "license": "GPLv3",
  "version": "1.1",
  "rules": [
    {
      "pattern": "~E",
      "replacement": "Ə"
    },
    {
      "pattern": "~e",
      "replacement": "ə"
    },
    {
      "pattern": "~h",
      "replacement": "ʰ"
    },
    {
      "pattern": "~H",
      "replacement": "ʰ"
    },
    {
      "pattern": "~L",
      "replacement": "Ɬ"
    },
    {
      "pattern": "~l",
      "replacement": "ɬ"
    },
    {
      "pattern": "~X",
      "replacement": "X̣"
    },
    {
      "pattern": "~x",
      "replacement": "x̣"
    },
    {
      "pattern": "~\\'",
      "replacement": "’"
    },
    {
      "pattern": "~\\?",
      "replacement": "ʔ"
    }
  ]
}