{
  "id": "yo-tilde",
  "name": "yo-tilde",
  "description": "Yoruba input keyboard - tilde",
  "date": "2018-11-30",
  "author": "Amir E. Aharoni",
  "license": "GPLv3",
  "version": "1.0",
  "rules": [
    {
      "pattern": "~E",
      "replacement": "Ẹ"
    },
    {
      "pattern": "~e",
      "replacement": "ẹ"
    },
    {
      "pattern": "~O",
      "replacement": "Ọ"
    },
    {
      "pattern": "~o",
      "replacement": "ọ"
    },
    {
      "pattern": "~S",
      "replacement": "Ṣ"
    },
    {
      "pattern": "~s",
      "replacement": "ṣ"
    },
    {
      "pattern": "~\\\\",
      "replacement": "̀"
    },
    {
      "pattern": "~/",
      "replacement": "́"
    },
    {
      "pattern": "~\\^",
      "replacement": "̂"
    },
    {
      "pattern": "~\\-",
      "replacement": "̄"
    },
    {
      "pattern": "~v",
      "replacement": "̌"
    }
  ]
}