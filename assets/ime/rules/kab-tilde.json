{
  "id": "kab-tilde",
  "name": "kab-tilde",
  "description": "Kabyle Latin tilde keyboard",
  "date": "2018-11-30",
  "author": "Amir E. Aharoni",
  "license": "GPLv3",
  "version": "1.0",
  "rules": [
    {
      "pattern": "~C",
      "replacement": "Č"
    },
    {
      "pattern": "~c",
      "replacement": "č"
    },
    {
      "pattern": "~D",
      "replacement": "Ḍ"
    },
    {
      "pattern": "~d",
      "replacement": "ḍ"
    },
    {
      "pattern": "~E",
      "replacement": "Ɛ"
    },
    {
      "pattern": "~e",
      "replacement": "ɛ"
    },
    {
      "pattern": "~G",
      "replacement": "Ǧ"
    },
    {
      "pattern": "~g",
      "replacement": "ǧ"
    },
    {
      "pattern": "~Y",
      "replacement": "Ɣ"
    },
    {
      "pattern": "~y",
      "replacement": "ɣ"
    },
    {
      "pattern": "~H",
      "replacement": "Ḥ"
    },
    {
      "pattern": "~h",
      "replacement": "ḥ"
    },
    {
      "pattern": "~S",
      "replacement": "Ṣ"
    },
    {
      "pattern": "~s",
      "replacement": "ṣ"
    },
    {
      "pattern": "~T",
      "replacement": "Ṭ"
    },
    {
      "pattern": "~t",
      "replacement": "ṭ"
    },
    {
      "pattern": "~Z",
      "replacement": "Ẓ"
    },
    {
      "pattern": "~z",
      "replacement": "ẓ"
    },
    {
      "pattern": "~w",
      "replacement": "ʷ"
    },
    {
      "pattern": "~v",
      "replacement": "̌"
    },
    {
      "pattern": "~\\.",
      "replacement": "̣"
    },
    {
      "pattern": "~,",
      "replacement": "̧"
    }
  ]
}