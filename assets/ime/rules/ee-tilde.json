{
  "id": "ee-tilde",
  "name": "ee-tilde",
  "description": "Ewe input keyboard",
  "date": "2018-11-30",
  "author": "Amir E. Aharoni",
  "license": "GPLv3",
  "version": "1.1",
  "rules": [
    {
      "pattern": "~D",
      "replacement": "Ɖ"
    },
    {
      "pattern": "~d",
      "replacement": "ɖ"
    },
    {
      "pattern": "~E",
      "replacement": "Ɛ"
    },
    {
      "pattern": "~e",
      "replacement": "ɛ"
    },
    {
      "pattern": "~F",
      "replacement": "Ƒ"
    },
    {
      "pattern": "~f",
      "replacement": "ƒ"
    },
    {
      "pattern": "~G",
      "replacement": "Ɣ"
    },
    {
      "pattern": "~g",
      "replacement": "ɣ"
    },
    {
      "pattern": "~N",
      "replacement": "Ŋ"
    },
    {
      "pattern": "~n",
      "replacement": "ŋ"
    },
    {
      "pattern": "~O",
      "replacement": "Ɔ"
    },
    {
      "pattern": "~o",
      "replacement": "ɔ"
    },
    {
      "pattern": "~V",
      "replacement": "Ʋ"
    },
    {
      "pattern": "~v",
      "replacement": "ʋ"
    },
    {
      "pattern": "~/",
      "replacement": "́"
    },
    {
      "pattern": "~\\\\",
      "replacement": "̀"
    },
    {
      "pattern": "~\\{",
      "replacement": "̃"
    },
    {
      "pattern": "~-",
      "replacement": "̄"
    },
    {
      "pattern": "~\\^",
      "replacement": "̌"
    }
  ]
}