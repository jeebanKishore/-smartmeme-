{
  "id": "ddn-tilde",
  "name": "ddn-tilde",
  "description": "Dendi input keyboard",
  "date": "2023-04-03",
  "author": "Amir E. Aharoni",
  "license": "GPLv3",
  "version": "1.0",
  "rules": [
    {
      "pattern": "~E",
      "replacement": "Ɛ"
    },
    {
      "pattern": "~e",
      "replacement": "ɛ"
    },
    {
      "pattern": "~N",
      "replacement": "Ŋ"
    },
    {
      "pattern": "~n",
      "replacement": "ŋ"
    },
    {
      "pattern": "~O",
      "replacement": "Ɔ"
    },
    {
      "pattern": "~o",
      "replacement": "ɔ"
    },
    {
      "pattern": "~\\\\",
      "replacement": "̀"
    },
    {
      "pattern": "~/",
      "replacement": "́"
    },
    {
      "pattern": "~-",
      "replacement": "̄"
    }
  ]
}