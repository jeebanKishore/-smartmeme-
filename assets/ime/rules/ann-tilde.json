{
  "id": "ann-tilde",
  "name": "ann-tilde",
  "description": "Obolo input keyboard",
  "date": "2020-11-03",
  "author": "Amir E. Aharoni",
  "license": "GPLv3",
  "version": "1.1",
  "rules": [
    {
      "pattern": "~N",
      "replacement": "N̄"
    },
    {
      "pattern": "~n",
      "replacement": "n̄"
    },
    {
      "pattern": "~O",
      "replacement": "Ọ"
    },
    {
      "pattern": "~o",
      "replacement": "ọ"
    },
    {
      "pattern": "~S",
      "replacement": "Ṣ"
    },
    {
      "pattern": "~s",
      "replacement": "ṣ"
    },
    {
      "pattern": "~\\\\",
      "replacement": "̀"
    },
    {
      "pattern": "~/",
      "replacement": "́"
    },
    {
      "pattern": "~\\^",
      "replacement": "̂"
    },
    {
      "pattern": "~-",
      "replacement": "̄"
    },
    {
      "pattern": "~v",
      "replacement": "̌"
    },
    {
      "pattern": "~\\.",
      "replacement": "̣"
    }
  ]
}