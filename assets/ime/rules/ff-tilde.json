{
  "id": "ff-tilde",
  "name": "Fulfulde tilde",
  "description": "Fulfulde tilde",
  "date": "2019-05-07",
  "author": "Amir E. Aharoni",
  "license": "GPLv3",
  "version": "1.0",
  "rules": [
    {
      "pattern": "~b",
      "replacement": "ɓ"
    },
    {
      "pattern": "~B",
      "replacement": "Ɓ"
    },
    {
      "pattern": "~d",
      "replacement": "ɗ"
    },
    {
      "pattern": "~D",
      "replacement": "Ɗ"
    },
    {
      "pattern": "~g",
      "replacement": "ɠ"
    },
    {
      "pattern": "~G",
      "replacement": "Ɠ"
    },
    {
      "pattern": "~q",
      "replacement": "ŋ"
    },
    {
      "pattern": "~Q",
      "replacement": "Ŋ"
    },
    {
      "pattern": "~j",
      "replacement": "ɲ"
    },
    {
      "pattern": "~J",
      "replacement": "Ɲ"
    },
    {
      "pattern": "~n",
      "replacement": "ñ"
    },
    {
      "pattern": "~N",
      "replacement": "Ñ"
    },
    {
      "pattern": "~y",
      "replacement": "ƴ"
    },
    {
      "pattern": "~Y",
      "replacement": "Ƴ"
    }
  ]
}